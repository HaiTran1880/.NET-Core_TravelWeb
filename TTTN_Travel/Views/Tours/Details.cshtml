<section class="tour-read-last">
    <div class="container">
        <div class="row">
            <div class="title-lq">Tour du lịch tương tự</div>

            @foreach (var i in ViewBag.listTour)
            {
                <div class="col-md-4 space-row">
                    <div class="row">
                        <div class="col-md-4">
                            <a href="Details?Id=@i.Idtour">
                                <img class="full-size" src="~/images/@i.Image">
                            </a>
                        </div>
                        <div class="col-md-8">
                            <div class="box-news">
                                <div class="title-h2">
                                    <a href="Details?Id=@i.Idtour">
                                        <span>@i.Tentuor</span>
                                    </a>
                                </div>
                                @if (i.Danhgia != 0)
                                {
                                    @for (var k = 1; k <= i.Danhgia; k++)
                                    {
                                        <i class="fa fa-star"></i>
                                    }
                                }
                                <h4><span>3.900.000</span> <b>@i.Gia Vnđ</b></h4>

                            </div>
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
</section>